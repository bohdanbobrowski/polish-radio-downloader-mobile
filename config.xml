<?xml version='1.0' encoding='utf-8'?>
<widget id="pl.com.bobrowski.prdl" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Polish Radio Downloader</name>
    <description />
    <author email="bohdan@bobrowski.com.pl" href="http://bobrowski.com.pl" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
    <plugin name="com.plugins.shortcut" spec="~0.0.2" />
    <plugin name="cordova-plugin-curl" spec="^1.0.1" />
    <plugin name="cordova-plugin-http" spec="^1.2.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="phonegap-plugin-push" spec="^1.10.4">
        <variable name="SENDER_ID" value="85075801930" />
    </plugin>
</widget>
